<%= link_to 'Wifi Users', wifiusers_path %>
<%= link_to 'Consos Wifi', wificonsos_path %>
<%= link_to 'Trafic Gowex', traficgowexes_path %>
<%= link_to 'Tweets', tweets_path %>

<h1>Listing traficgowexes</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Hour</th>
      <th>Average receive</th>
      <th>Min receive</th>
      <th>Max receive</th>
      <th>Average transmit</th>
      <th>Min transmit</th>
      <th>Max transmit</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @traficgowexes.each do |traficgowex| %>
      <tr>
        <td><%= traficgowex.date %></td>
        <td><%= traficgowex.hour %></td>
        <td><%= traficgowex.avrcv %></td>
        <td><%= traficgowex.minrcv %></td>
        <td><%= traficgowex.maxrcv %></td>
        <td><%= traficgowex.avtr %></td>
        <td><%= traficgowex.mintr %></td>
        <td><%= traficgowex.maxtr %></td>
        <td><%= link_to 'Show', traficgowex %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
